<div class='center jumbotron'>
  <h2>New group</h2>
  <%=form_for @group do |f|%>
    <div class="field">
      <%=f.label :name%>
      <%=f.text_field :name%>
    </div>
    
    <div class="field">
      <fieldset>
        <%= f.label :icon, 'icon' %>
        <%= f.hidden_field :icon_cache %>
        <%= f.file_field :icon%>
      </fieldset>
    </div>
    
    <div class="actions">
      <%=f.submit "Create", :class => 'btn btn-primary btn-lg'%>
    </div>  
  <%end%> 
</div>